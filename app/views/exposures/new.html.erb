<h1>New exposure</h1>

<% form_for(@exposure) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :study_id %><br />
    <%= f.text_field :study_id %>
  </p>
  <p>
    <%= f.label :klass %><br />
    <%= f.text_field :klass %>
  </p>
  <p>
    <%= f.label :relations %><br />
    <%= f.text_area :relations %>
  </p>
  <p>
    <%= f.label :windows %><br />
    <%= f.text_area :windows %>
  </p>
  <p>
    <%= f.label :types %><br />
    <%= f.text_area :types %>
  </p>
  <p>
    <%= f.label :assessments %><br />
    <%= f.text_area :assessments %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', exposures_path %>