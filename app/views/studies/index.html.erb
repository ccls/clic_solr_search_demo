<h1>Listing studies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>World region</th>
    <th>Subregion</th>
    <th>Country</th>
    <th>Study design</th>
    <th>Ascertainment</th>
    <th>Age group</th>
    <th>Recruitment</th>
    <th>Method Cytogenetic Subtyping</th>
    <th>Location Cytogenetic Subtyping</th>
    <th>Overview</th>
  </tr>

<% @studies.each do |study| %>
  <tr>
    <td><%=h study.study_name %></td>
    <td><%=h study.world_region %></td>
    <td><%=h study.subregion %></td>
    <td><%=h study.country %></td>
    <td><%=h study.study_design %></td>
    <td><%=h study.ascertainment %></td>
    <td><%=h study.age_group %></td>
    <td><%=h study.recruitment %></td>
    <td><%#=h study.method_cytogenetic_subtyping %></td>
    <td><%#=h study.location_cytogenetic_subtyping %></td>
    <td><%=h study.overview %></td>
    <td><%= link_to 'Show', study %></td>
    <td><%= link_to 'Edit', edit_study_path(study) %></td>
    <td><%= link_to 'Destroy', study, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
